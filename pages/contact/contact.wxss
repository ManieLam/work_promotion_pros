/* pages/contact/contact.wxss */

.container {
    padding: 30rpx 30rpx 0;
    overflow: hidden;
}

.contactCell {
    margin: 20rpx 0;
    padding: 20rpx 0;
}

.contactBtn {
    border-radius: 5px;
    border: 1rpx solid #e94c32;
    color: #e94c32;
    font-weight: 500;
    background: #fff;
    line-height: 80rpx;
}

.content {
    font-size: 14px;
}

.icon-content {
    background: url('../assets/images/icon-content.png') no-repeat;
    background-size: cover;
    width: 20px;
    height: 20px;
}

.btn_icon {
    border-radius: 5px;
    text-align: center;
    padding: 2% 14px;
}

.bg-2A95BF {
    background: #2A95BF;
    color: #fff;
}

.icon_block {
    display: inline-block;
    background-size: 100% 100%;
    /*background: #515151;*/
    background-image: url('data:image/png;base64,');
    margin: 0 5px;
}

.icon_smile {
    display: inline-block;
    background-image: url('data:image/png;base64,');
    background-size: 100% 100%;
    margin-right: 10rpx;
}

.lh2 {
    line-height: 2;
}

.cells_header {
    line-height: 120rpx;
    text-align: center;
    font-size: 22px;
}

.disInline {
    display: inline-block;
}

.justAround {
    justify-content: space-around;
}

.btn_call {
    margin-top: 5px;
    padding: 0 .5em !important;
}

.pd_l5 {
    padding-left: 5rpx;
}

.cell_name {
    width: 30%;
    font-weight: 600
}

.icons {
    width: 35rpx;
    height: 35rpx;
    min-width: 35rpx;
    vertical-align: middle;
}

.icons_sq {
    width: 38rpx;
    height: 35rpx;
    min-width: 38rpx;
    vertical-align: middle;
}

.mar_r5 {
    margin-right: 5px;
}

.mar_r10 {
    margin-right: 10rpx;
}

.newCells {
    padding: 10px 8px;
    position: relative;
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
}

.newCell_box {
    border: 1rpx solid #b2b2b2;
    margin: 5px 0;
}