/*分界线*/

.dividing-line {
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
    /*padding-bottom:20px;*/
    line-height: 100rpx;
    font-size: 17px;
}

.dividing-line::before,
.dividing-line::after {
    content: "";
    position: absolute;
    display: -webkit-flex;
    display: flex;
    width: 35%;
    height: 1px;
    background-color: #eee;
    /*top:25%;*/
    bottom: 45%;
}

.dividing-line::before {
    left: 2%;
}

.dividing-line::after {
    right: 2%;
}


