page {
    width: 100%;
    height: 100%;
    position: relative;
    color: #333;
    background-color: #f4f4f4;
    font-weight: 300;
    line-height: 1.6;
    font-family: PingFang, PingFang SC, Helvetica, Arial, Hiragino Sans GB, Microsoft Yahei, \\5FAE\8F6F\96C5\9ED1, STHeiti, \\534E\6587\7EC6\9ED1, sans-serif;
    -ms-text-size-adjust: 100%;
    -webkit-text-size-adjust: 100%;
    -webkit-tap-highlight-color: transparent;
}

